															<xs:element name="ID_SUJETO" type="ID_SECUENCIALType">
																<xs:annotation>
																	<xs:documentation>Identificador del sujeto pasivo.</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="IMPORTE">
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="INFORMA_RECARGO" type="xs:boolean">
																			<xs:annotation>
																				<xs:documentation>Indica si el ayuntamiento devuelve los importes aplicando recargos en caso que sean aplicables</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="PORCENTAJE_RECARGO" type="PORCENTAJEType" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Porcentaje aplicado</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="IMPORTE_RECARGO" type="IMPORTE_ECONOMICOType" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Importe del recargo</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="IMPORTE_TOTAL" type="IMPORTE_ECONOMICOType">
																			<xs:annotation>
																				<xs:documentation>Importe total satisfecho por sujeto pasivo incluyendo los recargos que informe el ayuntamiento</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
															<xs:element name="PDF">
																<xs:annotation>
																	<xs:documentation>Contenido del documento PDF en base64</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:base64Binary">
																		<xs:minLength value="1"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="CODIGO_JUSTIFICANTE_PAGO" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Codigo justificante del proceso de pago para este importe</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="22"/>
																		<xs:minLength value="1"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:element name="RESULTADO">
									<xs:annotation>
										<xs:documentation>Resultado de la petición . Los datos del cálculo no seran obligatorios si el retorno es distinto de cero.</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="RETORNO" type="xs:boolean">
												<xs:annotation>
													<xs:documentation>Se indicará 0 si la operación ha sido completada correctamente. En caso contrario se indicará 1.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ERROR" minOccurs="0">
												<xs:annotation>
													<xs:documentation>En caso de error se indicará el código y la descripción.</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="CODIGO">
															<xs:annotation>
																<xs:documentation>Código del error pactado entre el organismo de gestión tributaria y el CGN.</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="STRINGType">
																	<xs:length value="4"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:element>
														<xs:element name="DESCRIPCION">
															<xs:annotation>
																<xs:documentation>Descripción del error.</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="STRINGType">
																	<xs:minLength value="1"/>
																	<xs:maxLength value="255"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EXENCIONES_BONIFICACIONES">
		<xs:annotation>
			<xs:documentation>Mensaje de intercambio sincrono que permite obtener las bonificaciones y exenciones disponibles.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element name="REQUEST">
						<xs:annotation>
							<xs:documentation>Interrogación de bonificaciones y exenciones enviado desde el CGN</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="CABECERA" type="CabeceraType">
									<xs:annotation>
										<xs:documentation>Cabecera del mensaje.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="INE_MUNICIPIO" type="CODIGO_MUNICIPIOType"/>
								<xs:element name="INE_PROVINCIA" type="CODIGO_PROVINCIAType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="REPLY">
						<xs:annotation>
							<xs:documentation>Respuesta con las bonificaciones y exenciones enviado por el organismo de gestión tributaria.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="CABECERA" type="CabeceraType">
									<xs:annotation>
										<xs:documentation>Cabecera del mensaje.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:sequence minOccurs="0">
									<xs:element name="BONIFICACIONES" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="BONIFICACION" maxOccurs="unbounded">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="PORCENTAJE" type="PORCENTAJEType"/>
															<xs:element name="CONCEPTO" type="xs:string"/>
															<xs:element name="DESCRIPCION" type="xs:string"/>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="EXENCIONES" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="EXENCION" maxOccurs="unbounded">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="PORCENTAJE" type="PORCENTAJEType"/>
															<xs:element name="CONCEPTO" type="xs:string"/>
															<xs:element name="DESCRIPCION" type="xs:string"/>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:element name="RESULTADO">
									<xs:annotation>
										<xs:documentation>Resultado de la petición . Los datos del cálculo no seran obligatorios si el retorno es distinto de cero.</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="RETORNO" type="xs:boolean">
												<xs:annotation>
													<xs:documentation>Se indicará 0 si la operación ha sido completada correctamente. En caso contrario se indicará 1.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ERROR" minOccurs="0">
												<xs:annotation>
													<xs:documentation>En caso de error se indicará el código y la descripción.</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="CODIGO">
															<xs:annotation>
																<xs:documentation>Código del error pactado entre el organismo de gestión tributaria y el CGN.</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="STRINGType">
																	<xs:length value="4"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:element>
														<xs:element name="DESCRIPCION">
															<xs:annotation>
																<xs:documentation>Descripción del error.</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="STRINGType">
																	<xs:minLength value="1"/>
																	<xs:maxLength value="255"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="CabeceraType">
		<xs:sequence>
			<xs:element name="APLICACION" type="xs:string" fixed="PLV">
				<xs:annotation>
					<xs:documentation>Campo fijo con valor PLV</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ID_COMUNICACION" type="xs:long">
				<xs:annotation>
					<xs:documentation>identifica una comunicacion. REQUEST y REPLY deben usar el mismo valor</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FECHA" type="TFecha">
				<xs:annotation>
					<xs:documentation>Fecha de la comunicacion</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HORA" type="THora">
				<xs:annotation>
					<xs:documentation>Hora de la comunicación</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OPERACION">
				<xs:annotation>
					<xs:documentation>Código de la Operación que se desea Realizar. "01" CALCULO, "02" PAGO, "03" EXENCIONES-BONIFICACIONES</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="01"/>
						<xs:enumeration value="02"/>
						<xs:enumeration value="03"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="EMISOR" type="xs:string">
				<xs:annotation>
					<xs:documentation>Código representativo de la entidad emisora (CGN para el REQUEST y código INE para el REPLY)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RECEPTOR">
				<xs:annotation>
					<xs:documentation>Código de la entidad Receptora (Código INE para el REQUEST, y CGN para el REPLY)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:whiteSpace value="collapse"/>
						<xs:minLength value="3"/>
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SujetoType">
		<xs:sequence>
			<xs:element name="IDENTIFICADOR" type="ID_SECUENCIALType">
				<xs:annotation>
					<xs:documentation>Secuencial que identifica al sujeto de forma unica en el documento.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DATOS_PERSONALES">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TIPO_DOCUMENTO" type="TIPO_DOCType">
							<xs:annotation>
								<xs:documentation>Tipo de documento de identidad ("1" NIF/CIF, "2" NIE, "3" Pasaporte, "4" Menores de edad que carezcan de dicho número, "5" Entidad recién constituida que carece de NIF, "6" Falta de aportación del NIF o NIF aportado pero no válido, "7" Cónyuge cotitular que no interviene, "8" Cédula de identificación de extranjero, "9" Número de identificación de persona jurídica extranjera)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NUMERO_DOCUMENTO" type="NUMERO_DOCUMENTO_IDENTIDADType">
							<xs:annotation>
								<xs:documentation>Número de documento de identidad.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NOMBRE" type="NOMBRE_PERSONAType" minOccurs="0"/>
						<xs:element name="APELLIDO1_RAZONSOCIAL" type="APELLIDOS_PERSONAType"/>
						<xs:element name="APELLIDO2" type="APELLIDOS_PERSONAType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:choice>
				<xs:element name="DOMICILIO" type="DireccionType">
					<xs:annotation>
						<xs:documentation>Domicilio en caso de residentes.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DOMICILIO_EXTRANJERO" type="DireccionExtranjeraType">
					<xs:annotation>
						<xs:documentation>Domicilio en el caso que no tengan dirección de residencia en España.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DireccionType">
		<xs:annotation>
			<xs:documentation>Dirección en España</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="INE_PROVINCIA" type="CODIGO_PROVINCIAType">
				<xs:annotation>
					<xs:documentation>Codigo INE de Provincia</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="INE_MUNICIPIO" type="CODIGO_MUNICIPIOType">
				<xs:annotation>
					<xs:documentation>Codigo INE de Municipio. (incluye digito de control)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TIPO_VIA" type="TIPO_VIAType">
				<xs:annotation>
					<xs:documentation>Tipo de vía, según la codificación de Catastro</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VIA" type="NOMBRE_VIAType">
				<xs:annotation>
					<xs:documentation>Nombre de la via</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NUMERO_VIA" type="NUMERO_VIAType" minOccurs="0"/>
			<xs:element name="ESCALERA" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="STRINGType">
						<xs:maxLength value="2"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PLANTA" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="STRINGType">
						<xs:minLength value="1"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PUERTA" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="STRINGType">
						<xs:minLength value="1"/>
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DUPLICADO" type="DUPLICADOType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Duplicado ("D", "T", "C")</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BLOQUE" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bloque</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="STRINGType">
						<xs:maxLength value="4"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="APR_KM" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aproximación postal en kilometros</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ENTIDAD_MENOR" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Entidad local menor o pedanía</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="STRINGType">
						<xs:maxLength value="15"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="RESTO" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Resto de dirección no estructurada</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="STRINGType">
						<xs:maxLength value="25"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CODIGO_POSTAL" type="CODIGO_POSTALType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DireccionExtranjeraType">
		<xs:sequence>
			<xs:element name="PAIS" type="CODIGO_PAISType">
				<xs:annotation>
					<xs:documentation>Código de país, según la codificación oficial de Países y Territorios aprobada por OM 9-12-99</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EST_PRV" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Estado o provincia</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="STRINGType">
						<xs:maxLength value="40"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DESC_MUNICIPIO">
				<xs:annotation>
					<xs:documentation>Nombre del municipio</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="POBLACIONType">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DIRECCION_NO_ESTRUCTURADA">
				<xs:annotation>
					<xs:documentation>Dirección completa de residencia en formato texto libre.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="STRINGType">
						<xs:minLength value="1"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DISTRITO_POSTAL" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Distrito postal</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="STRINGType">
						<xs:maxLength value="10"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TransmitenteType">
		<xs:sequence>
			<xs:element name="DATOS_BASICOS" type="SujetoType"/>
			<xs:element name="PORCENTAJE_TRANSMITIDO" type="PORCENTAJEType">
				<xs:annotation>
					<xs:documentation>Porcentaje transmitido sobre el total de la finca.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CLASE_DERECHO" type="CLASE_DERECHOType">
				<xs:annotation>
					<xs:documentation>Clase de derecho transmitido o constituido mediante la operación ("1" Pleno dominio, "2" Nuda Propiedad, "3" Usufructo vitalicio, "4" Uso o habitación temporal, "5" Derecho de superficie, "6" Concesión administrativa, "7" Usufructo temporal, "8" Uso o habitación vitalicio)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FECHA_DEFUNCION" type="TFecha" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Fecha de defunción del causante de heréncia. Será la fecha de transmisión en transmisiones Mortis causa.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EDAD_USUFRUCTUARIO" type="ANYOSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Edad del usufructuario. Obligatorio en usufructos vitalicios.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ANYOS_USUFRUCTO" type="ANYOSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Años de usufructo. Obligatorio en usufructos temporales.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EDAD_USO" type="ANYOSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Años de uso. Obligatorio en Uso o habitación vitalicio.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ANYOS_USO" type="ANYOSType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Años de uso. Obligatorio en Uso o habitación temporal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TRANSMISIONES_ANTERIORES">
				<xs:annotation>
					<xs:documentation>Cada una de las transmisiones anteriores. (fechas y porcentajes de adquisición del bien transmitido)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TRANSMISION" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="FECHA" type="TFecha">
										<xs:annotation>
											<xs:documentation>Fecha de adquisición</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="PORCENTAJE" type="PORCENTAJEType">
										<xs:annotation>
											<xs:documentation>Porcentaje adquirido sobre el total transmitido.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="VALOR" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Precio de la adiquisición anterior</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:decimal">
												<xs:totalDigits value="15"/>
												<xs:fractionDigits value="2"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
